{
    "name": "eventix/laravel-translation",
    "description": "The Eventix Translation package based on the Laravel one.",
    "homepage": "https://eventix.io",
    "authors": [
        {
            "name": "Mart Pluijmaekers",
            "email": "mart@eventix.io"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "5.8.*",
        "illuminate/support": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Eventix\\Translation\\": ""
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eventix\\Translation\\TranslationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
